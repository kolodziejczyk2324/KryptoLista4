{
"manifest_version": 2,
"name": "Aplikacja do zmiany adresu w banku na krypto",
"description": "krypto zadanie",
"version": "1.2",

 "browser_action": {
  "default_title": "Krypto"
 },
 "permissions": [
 "background",
 "alarms",
 "activeTab",
 "clipboardRead",
